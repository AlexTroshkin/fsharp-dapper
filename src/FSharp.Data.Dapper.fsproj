<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Authors>Alex Troshkin</Authors>    
    <Description>The wrapper above the 'Dapper' library allows you to write more familiar code in the F # language. It also contains a functional for more simple work with temporary tables</Description>
    
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/AlexTroshkin/fsharp-dapper</RepositoryUrl>
    <PackageProjectUrl>https://github.com/AlexTroshkin/fsharp-dapper</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/AlexTroshkin/fsharp-dapper/blob/master/LICENCE</PackageLicenseUrl>
    <PackageTags>fsharp dapper</PackageTags>
    
    <!--<IncludeSymbols>true</IncludeSymbols>
    <IncludeSource>true</IncludeSource>-->
    
    <Version>1.0.3</Version>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>
    
  <ItemGroup>
    <Compile Include="Enums.fs" />
    <Compile Include="SqlTypeRepresentation.fs" />
    <Compile Include="SqlClrTypeCorrelation.fs" />
    <Compile Include="Reflection.fs" />
    <Compile Include="TemporaryTable.fs" />
    <Compile Include="Query.fs" />
  </ItemGroup>
  <Import Project="..\.paket\Paket.Restore.targets" />
</Project>