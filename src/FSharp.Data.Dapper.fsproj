<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>The wrapper above the 'Dapper' library allows you to write more familiar code in the F # language. It also contains a functional for more simple work with temporary tables</Description>
    <PackageProjectUrl>https://github.com/AlexTroshkin/fsharp-dapper</PackageProjectUrl>
    <RepositoryUrl>https://github.com/AlexTroshkin/fsharp-dapper</RepositoryUrl>
    <PackageTags>fsharp dapper</PackageTags>
    <Version>1.0.3</Version>
    <Authors>Alex Troshkin</Authors>
    <Company />
    <FileVersion>1.0.0.3</FileVersion>
    <AssemblyVersion>1.0.0.3</AssemblyVersion>
    <PackageReleaseNotes>Closed issue #1 - https://github.com/AlexTroshkin/fsharp-dapper/issues/1
    Provide a way for work with temporary table when collection contains primitive type</PackageReleaseNotes>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="Reflection.fs" />
    <Compile Include="SqlTypeMapping.fs" />
    <Compile Include="TemporaryTable.fs" />
    <Compile Include="Query.fs" />
  </ItemGroup>
  <Import Project="..\.paket\Paket.Restore.targets" />
</Project>